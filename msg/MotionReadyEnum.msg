# Result codes which may be returned by services and actions which attempt to
# execute motion (or are executing motion).
#
# Values other than 1 would indicate an active error or other active inhibition
# on the Yaskawa controller preventing MotoROS2 from enabling trajectory mode
# or executing any motion.
#
# Applications are encouraged to refer to the named constants generated by
# rosidl as part of this message instead of using the integer values directly.

uint16 READY=1
string READY_STR="Ready"

uint16 NOT_READY_UNSPECIFIED=100
string NOT_READY_UNSPECIFIED_STR="Unspecified"

uint16 NOT_READY_ALARM=101
string NOT_READY_ALARM_STR="The controller has an active Alarm"

uint16 NOT_READY_ERROR=102
string NOT_READY_ERROR_STR="The controller has an active Error"

uint16 NOT_READY_ESTOP=103
string NOT_READY_ESTOP_STR="The controller is in E-Stop"

uint16 NOT_READY_NOT_PLAY=104
string NOT_READY_NOT_PLAY_STR="The teach pendant must not be in TEACH mode"

uint16 NOT_READY_NOT_REMOTE=105
string NOT_READY_NOT_REMOTE_STR="The teach pendant must be in REMOTE mode"

uint16 NOT_READY_SERVO_OFF=106
string NOT_READY_SERVO_OFF_STR="Servo power is OFF. Please call start_traj_mode or start_point_queue_mode service"

uint16 NOT_READY_HOLD=107
string NOT_READY_HOLD_STR="The controller is in HOLD"

uint16 NOT_READY_JOB_NOT_STARTED=108
string NOT_READY_JOB_NOT_STARTED_STR="The INIT_ROS job has not started. Please call start_traj_mode or start_point_queue_mode service"

uint16 NOT_READY_NOT_ON_WAIT_CMD=109
string NOT_READY_NOT_ON_WAIT_CMD_STR="INFORM job is not on a WAIT command. Check the format of INIT_ROS"

uint16 NOT_READY_PFL_ACTIVE=111
string NOT_READY_PFL_ACTIVE_STR="A PFL event has occurred. Please return the robot to a safe state"

uint16 NOT_READY_INC_MOVE_ERROR=112
string NOT_READY_INC_MOVE_ERROR_STR="The controller is in an error state. Please call reset_error to attempt to clear the error"

uint16 NOT_READY_OTHER_PROGRAM_RUNNING=113
string NOT_READY_OTHER_PROGRAM_RUNNING_STR="Controller is running another job"

uint16 NOT_READY_OTHER_TRAJ_MODE_ACTIVE=114
string NOT_READY_OTHER_TRAJ_MODE_ACTIVE_STR="Another trajectory mode is already active. Please call stop_traj_mode service"

uint16 NOT_READY_NOT_CONT_CYCLE_MODE=115
string NOT_READY_NOT_CONT_CYCLE_MODE_STR="Continuous cycle mode not set. Please set the cycle mode to continuous"

uint16 value

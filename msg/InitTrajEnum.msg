# Result codes which may be returned when attempting to initialize trajectory
#
# Values other than 0 would indicate an active error or other active inhibition
# on the Yaskawa controller preventing MotoROS2 from initializing a trajectory
#
# Applications are encouraged to refer to the named constants generated by
# rosidl as part of this message instead of using the integer values directly.

uint16 INIT_TRAJ_OK=0
string INIT_TRAJ_OK_STR="OK"

uint16 INIT_TRAJ_UNSPECIFIED=200
string INIT_TRAJ_UNSPECIFIED_STR="Unspecified failure"

uint16 INIT_TRAJ_TOO_SMALL=201
string INIT_TRAJ_TOO_SMALL_STR="Trajectory must contain at least two points."

uint16 INIT_TRAJ_TOO_BIG=202
string INIT_TRAJ_TOO_BIG_STR="Trajectory contains too many points (Not enough memory)."

uint16 INIT_TRAJ_ALREADY_IN_MOTION=203
string INIT_TRAJ_ALREADY_IN_MOTION_STR="Already running a trajectory."

uint16 INIT_TRAJ_INVALID_STARTING_POS=204
string INIT_TRAJ_INVALID_STARTING_POS_STR="The first point must match the robot's current position."

uint16 INIT_TRAJ_INVALID_VELOCITY=205
string INIT_TRAJ_INVALID_VELOCITY_STR="The commanded velocity is too high."

uint16 INIT_TRAJ_INVALID_JOINTNAME=206
string INIT_TRAJ_INVALID_JOINTNAME_STR="Invalid joint name specified. Check motoros2_config.yaml."

uint16 INIT_TRAJ_INCOMPLETE_JOINTLIST=207
string INIT_TRAJ_INCOMPLETE_JOINTLIST_STR="Trajectory must contain data for all joints."

uint16 INIT_TRAJ_INVALID_TIME=208
string INIT_TRAJ_INVALID_TIME_STR="Invalid time in trajectory."

uint16 INIT_TRAJ_WRONG_MODE=209
string INIT_TRAJ_WRONG_MODE_STR="Must call start_traj_mode service"

uint16 INIT_TRAJ_BACKWARD_TIME=210
string INIT_TRAJ_BACKWARD_TIME_STR="Trajectory message contains waypoints that are not strictly increasing in time."

uint16 INIT_TRAJ_WRONG_NUMBER_OF_POSITIONS=211
string INIT_TRAJ_WRONG_NUMBER_OF_POSITIONS_STR="Trajectory did not contain position data for all axes."

uint16 INIT_TRAJ_WRONG_NUMBER_OF_VELOCITIES=212
string INIT_TRAJ_WRONG_NUMBER_OF_VELOCITIES_STR="Trajectory did not contain velocity data for all axes."

uint16 INIT_TRAJ_INVALID_ENDING_VELOCITY=213
string INIT_TRAJ_INVALID_ENDING_VELOCITY_STR="The final point in the trajectory must have zero velocity."

uint16 INIT_TRAJ_INVALID_ENDING_ACCELERATION=214
string INIT_TRAJ_INVALID_ENDING_ACCELERATION_STR="The final point in the trajectory must have zero acceleration."

uint16 INIT_TRAJ_DUPLICATE_JOINT_NAME=215
string INIT_TRAJ_DUPLICATE_JOINT_NAME_STR="The trajectory contains duplicate joint names."

uint16 value
